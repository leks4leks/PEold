                <TextBlock Text="{Binding BusinessResults.OtherCapitalEnd, StringFormat={}{0:#,##0,}, ConverterCulture=ru-RU}" Grid.Row="18" Grid.Column="5" Style="{StaticResource TextBlock12CenterCenter}"/>
